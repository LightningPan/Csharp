var isClick = false;
$('.main-nav-trigger').click(function () {
    $('.main-nav-response-left').toggleClass('active');

    if (isClick === false) {
        isClick = true;
        $(window).scroll(function () {
            $('.main-nav-response-left').removeClass('active');
        });
    }
});
$('.community-index-tab').mouseenter(function () {
    $(this).find('.community-index-tab-static').css('display', 'none');
    $(this).find('.community-index-tab-dynamic').css('display','inline-block');
});
$('.community-index-tab').mouseleave(function () {
    $(this).find('.community-index-tab-static').css('display', 'inline-block');
    $(this).find('.community-index-tab-dynamic').css('display', 'none');
});
$('.main-head-user-setting').mouseover(function () {
    $('.main-head-user-setting ul').show();
});
$('.main-head-user-setting').mouseout(function () {
    $('.main-head-user-setting ul').hide();
});
seajs.use('login/passport');
