    var tip = {
        passport: '提示：与百度搜索、百度贴吧、百度云盘、百度知道、百度文库等产品通用。',
        uc: '提示：百度推广、百度网盟、百度联盟、百度统计、百度司南等账号在此登录。'
    };
    var baseUrl = location.protocol + '//' + location.host;
    ucCommonLogin.init({
        appid: 483,
        fromu: location.toString(),
        staticPage: baseUrl + '/casJump.html',
        isLayer: 1,
        defaultLogin: 'uc',
        multiLogin: 1,
        ucTitle: '百度商业账号',
        passportTitle: '百度账号',
        isSmall: 1,
        registerUcLink: 'https://u.baidu.com/ucweb/?module=Reguser&controller=reg&action=index&appid=3',
        // registerPassLink: 'https://passport.baidu.com/v2/?reg&u=' + escape(location.toString()),
        beforeSubmit: function(o) {
            _hmt.push(['_trackEvent', '登录', '点击', o ? o.type : 'UC']);
            if (globalConfig.fromu) {
                globalConfig.frame = window.open('about:blank', '_blank');
                globalConfig.frame.document.body.innerHTML = '登录中...';
            }
            return true;
        },
        success: function(o) {
            _hmt.push(['_trackEvent', '登录-成功', '登录', o.type]);
            if (globalConfig.fromu) {
                _hmt.push(['_trackEvent', `登录成功跳转-${globalConfig.fromPid}`, '登录', o.type]);
                globalConfig.frame.location = globalConfig.fromu;
            }
            location.replace(location.toString());
        },
        fail: function(o) {
            _hmt.push(['_trackEvent', '登录失败', '登录', o.type]);
            if (globalConfig.fromu) {
                globalConfig.frame.close();
            }
        },
        rendered: function() {
            $('.uc-register').on('click', function(){
                _hmt.push(['_trackEvent', '注册', '点击', 'UC']);
            });
            $('#common-login').append('<div class="tip-wrap"><div class="tip-info"></div></div>');
            $('#common-login .tip-info').text(tip.uc);
            $('#common-login').on('click', function (e) {
                var $target = $(e.target);
                var target = e.target;
                if ($target.closest('.change-login').length) {
                    if (target.innerText === '百度账号') {
                        $('#common-login .tip-info').text(tip.passport);
                    }
                    else if (target.innerText === '百度商业账号') {
                        $('#common-login .tip-info').text(tip.uc);
                    }
                }
            });
        }
    });
    ucCommonLogin.PubSub.on('hide', function() {
        globalConfig.fromu = null;
        globalConfig.frame = null;
        globalConfig.fromPid = null;
    });
    </script>
